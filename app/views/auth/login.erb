<h1>Login Page</h1>

<form method="post" action="/login">
  <div>
    <label>Username</label>
    <input type="text" name="username">
  </div>
  
  <div>
    <label>Email</label>
    <input type="text" name="email">
  </div>

  <div>
    <label>Password</label>
    <input type="password" name="password">
  </div>

  <div>
    <input type="submit" value="Submit">
  </div>
 
</form>